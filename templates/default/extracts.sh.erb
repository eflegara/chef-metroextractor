<% require 'json' -%>
<% json = File.read("#{node[:metroextractor][:setup][:scriptsdir]}/cities.json") -%>
<% data = JSON.parse(json) -%>
<% data['regions'].each do |region, val| -%>
  <% val['cities'].each do |city, val| %>
vex <%= node[:metroextractor][:vex][:db] %> "<%= val['bbox']['bottom'] %>" "<%= val['bbox']['left'] %>" "<%= val['bbox']['top'] %>" "<%= val['bbox']['right'] %>" -out ex/<%= city %>.osm.pbf
  <% end %>
<% end %>
